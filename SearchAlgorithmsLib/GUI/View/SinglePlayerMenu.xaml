<Window x:Class="GUI.View.SinglePlayerMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI.View"
        xmlns:controls="clr-namespace:GUI.Controls"
        mc:Ignorable="d"
        Title="SinglePlayerMenu" Height="180" Width="350">
    <Grid>
        <controls:DefinePlay Margin="0,0,0,45" MazeName="{Binding MazeName}"/>
        <Button x:Name="btnStart" HorizontalAlignment="Center" Click="btnStart_Click" Margin="136,110,123.333,9.667" Padding="5" Width="84">Start Game</Button>

    </Grid>
</Window>
